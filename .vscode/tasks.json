{
 "tasks": [
  {
   "type": "cppbuild",
   "label": "build",
   "command": "/usr/bin/gcc",
   "args": [
    "-fdiagnostics-color=always",
    "-g",
    "-include",
    "${workspaceFolder}/include/*.h",
    "${workspaceFolder}/include/*.c",
    "-include",
    "${workspaceFolder}/test/*.h",
    "${workspaceFolder}/test/*.c",
    "-o",
    "${fileDirname}/${fileBasenameNoExtension}.out"
   ],
   "options": {
    "cwd": "${workspaceFolder}"
   },
   "problemMatcher": [
    "$gcc"
   ],
   "detail": "Task generated by Debugger."
  }
 ],
 "version": "2.0.0"
}