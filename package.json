{
  "name": "@testing/string-number",
  "version": "0.0.0",
  "main": "./wasm/index.js",
  "module": "./wasm/index.js",
  "exports": {
    "import": "./wasm/index.js",
    "require": "./wasm/index.js"
  },
  "scripts": {
    "build:emcc-js": "mkdirp wasm && emcc include/*.c -sWASM=0 -sNO_EXIT_RUNTIME=1 -sMODULARIZE=1 -sDYNAMIC_EXECUTION=1 -sEXPORTED_RUNTIME_METHODS='[\"cwrap\",\"ccall\"]' -o ./wasm/index.js"
  },
  "license": "WTFPL",
  "dependencies": {
    "emscripten": "0.0.2-beta",
    "mkdirp": "^1.0.4",
    "typescript": "^4.8.4"
  }
}
